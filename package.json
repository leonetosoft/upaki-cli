{
  "name": "upaki-cli",
  "version": "1.0.0",
  "description": "Cliente Nodejs para comunicação com o Upaki",
  "main": "lib/index.js",
  "repository": "https://www.github.com/elboman/typescript-lib-example",
  "author": "Leonardo Neto",
  "license": "MIT",
  "scripts": {
    "pretest": "npm run clean && tsc",
    "clean": "shx rm -rf _bundles lib lib-esm",
    "lib": "npm run clean && tsc",
    "lib2": "npm run clean && tsc && tsc -m es6 --outDir lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack --watch --config webpack.config.js --progress --colors"
  },
  "dependencies": {
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "aws-sdk": "^2.213.1",
    "body-parser": "^1.18.2",
    "moment": "^2.20.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.83.0",
    "shx": "^0.2.2"
  },
  "devDependencies": {
    "webpack": "^2.2.1",
    "@types/body-parser": "^1.16.8",
    "@types/event-emitter": "^0.3.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/request": "^2.0.9",
    "@types/node": "^8.0.49",
    "typescript": "2.5.2",
    "ts-node": "3.3.0"
  }
}
